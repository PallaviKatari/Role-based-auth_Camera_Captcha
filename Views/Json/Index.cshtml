@using Role_based_auth.Models;
@model IEnumerable<CustomerModel>

@{
    ViewBag.Title = "Index";
}

<body>
    <table class="table table-dark">
        <tr>
            <th>CustomerId</th>
            <th>Name</th>
            <th>Country</th>
        </tr>
        @foreach (CustomerModel customer in Model)
        {
            <tr>
                <td>@customer.CustomerId</td>
                <td>@customer.Name</td>
                <td>@customer.Country</td>
            </tr>
        }
    </table>
</body>

